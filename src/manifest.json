{
  "name": "haproxy",
  "description": "Api proxy",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "/assets/icon.png"
  },
  "icons": {
    "64": "/assets/icon.png"
  },
  "options_page": "options.html",
  "permissions": ["storage", "activeTab", "scripting", "contextMenus", "webRequest", "webRequestBlocking"],
  "background": {
    "service_worker": "background.js"
  }
}
